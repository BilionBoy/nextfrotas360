<div class="container">
  <%= render "shared/notice" %>
  <div class="card border-0 shadow shadow-sm">
    <div class="card-header border-0 d-flex justify-content-between">
      <span class="fs-4">Visualizar F Financeiro Movimento</span>
    </div>
    <div class="card-body">
      <div id="<%= dom_id @f_financeiro_movimento %>">
          <p>
            <strong>Descricao:</strong>
            <%= @f_financeiro_movimento.descricao %>
          </p>
          <p>
            <strong>F Financeiro:</strong>
            <%= @f_financeiro_movimento.f_financeiro %>
          </p>
          <p>
            <strong>G Tipo Movimento:</strong>
            <%= @f_financeiro_movimento.g_tipo_movimento %>
          </p>
          <p>
            <strong>Valor:</strong>
            <%= @f_financeiro_movimento.valor %>
          </p>
          <p>
            <strong>Observacao:</strong>
            <%= @f_financeiro_movimento.observacao %>
          </p>
      </div>
    </div>
    <div class="card-footer border-0 d-flex justify-content-between">
      <div>
        <%= link_to edit_f_financeiro_movimento_path(@f_financeiro_movimento), class: "btn btn-primary" do %>
        <i class="bi bi-pencil-square"></i> Editar
        <% end %>
        <%= link_to "Voltar", f_financeiros_movimentos_path, class: "btn btn-secondary" %>
      </div>
      <%= button_to "Deletar", @f_financeiro_movimento, method: :delete, class: "btn btn-outline-danger border-0" %>
    </div>
  </div>
</div>
